{
	"name": "openapi-usage",
	"version": "0.0.0",
	"description": "OpenAPI仕様を正としてAPIの使用状況を静的解析し、未使用APIを検知するツール",
	"keywords": [
		"openapi",
		"api",
		"analyzer",
		"static-analysis",
		"typescript",
		"openapi-fetch",
		"unused-api",
		"cli"
	],
	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Glider2355/openapi-usage.git"
	},
	"bugs": {
		"url": "https://github.com/Glider2355/openapi-usage/issues"
	},
	"homepage": "https://github.com/Glider2355/openapi-usage#readme",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"openapi-usage": "dist/bin.js"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json && chmod +x dist/bin.js",
		"clean": "rm -rf dist",
		"prebuild": "pnpm clean",
		"prepublishOnly": "pnpm build",
		"test": "vitest",
		"test:run": "vitest run",
		"lint": "biome check .",
		"format": "biome check --write .",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"commander": "^13.1.0",
		"ts-morph": "^25.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@changesets/cli": "^2.27.0",
		"@types/node": "^25.0.8",
		"lefthook": "^2.0.15",
		"tsx": "^4.19.4",
		"typescript": "^5.9.2",
		"vitest": "^4.0.17"
	}
}
